
lbl0:
	MOV AX, HEAP_DAT 
	FPUSH AX 
	NEXT
lbl1:
	MOV AX, STR_DAT 
	FPUSH AX 
	NEXT
lbl2:
	FPOP BX 
	MOV AX, [BX] 
	FPUSH AX 
	NEXT
lbl3:
	FPOP BX 
	FPOP AX 
	MOV [BX], AX 
	NEXT
lbl6:	FPOP BX 
	XOR AX, AX 
	MOV AL, byte [BX] 
	FPUSH AX 
	NEXT
lbl7:	FPOP BX 
	FPOP AX 
	MOV byte [BX], AL 
	NEXT
lbl8:	MOV AX, MIR_DAT 
	FPUSH AX 
	NEXT
lbl10:	FPOP AX 
	JMP AX 
	NEXT
lbl11:	FPOP AX 
	FPOP BX 
	OR BX, BX 
	JNZ END_ZJMP 
	JMP AX 
END_ZJMP: 
	NEXT

FLBL0:
	dw 0
	db "&HERE",0
	CALL 0:lbl0
	NEXT
FLBL1:
	dw FLBL0
	db "&STR",0
	CALL 0:lbl1
	NEXT
FLBL2:
	dw FLBL1
	db "@",0
	CALL 0:lbl2
	NEXT
FLBL3:
	dw FLBL2
	db "!",0
	CALL 0:lbl3
	NEXT
FLBL4:
	dw FLBL3
	db "STR",0
lbl4:
	CALL 0:lbl1
	CALL 0:lbl2
	NEXT
FLBL5:
	dw FLBL4
	db "HERE",0
lbl5:
	CALL 0:lbl0
	CALL 0:lbl2
	NEXT
FLBL6:
	dw FLBL5
	db "C@",0
	CALL 0:lbl6
	NEXT
FLBL7:
	dw FLBL6
	db "C!",0
	CALL 0:lbl7
	NEXT
FLBL8:
	dw FLBL7
	db "&M",0
	CALL 0:lbl8
	NEXT
FLBL9:
	dw FLBL8
	db "M",0
lbl9:
	CALL 0:lbl8
	CALL 0:lbl2
	NEXT
FLBL10:
	dw FLBL9
	db "JMP",0
	CALL 0:lbl10
	NEXT
FLBL11:
	dw FLBL10
	db "ZJMP",0
	CALL 0:lbl11
	NEXT
